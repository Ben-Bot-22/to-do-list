{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["main.js"],
    "rules": {
        "max-len": "warn",
        "no-unused-vars": "warn",
        "no-trailing-spaces": "warn",
        "comma-dangle": "off",
        "spaced-comment": "warn",
        "prefer-const": "warn",
        "object-shorthand": "warn",
        "prefer-arrow-callback": "warn",
        "import/prefer-default-export": "warn",
    }
}
